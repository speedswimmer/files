/* Script to disconnect WiFi
Author: M.H.
*/
typingSpeed(0,0);
press("WIN R");
delay(500);
type("cmd");
delay(500);
press("ENTER")  //press("LEFT_CTRL LEFT_SHIFT ENTER");
delay(500);
type("netsh wlan show interfaces");
delay(200);
press("ENTER");
delay(500);
type("netsh wlan disconnect");
delay(200);
press("ENTER");
delay(1000);
press("ALT F4");
